---
export const prerender = false;
const response = await fetch(
  "https://api.tablebackend.com/v1/rows/1dQucBaX5vjr"
);
const posts = await response.json();

type Post = {
  id: number;
  title: string;
  date: string;
  body: string;
};
---

<div>
  {
    posts && (
      <ul>
        {posts.data.map((post: Post) => (
          <li>{post.title}</li>
        ))}
      </ul>
    )
  }
</div>
